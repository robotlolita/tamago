% language: tamago
namespace tamago::examples::fibonacci

use tamago::prelude exposing _ + _, _ - _, Integer;

define n fib =
begin
  match n with
  | 0 => 0;
  | 1 => 1;
  | n => (n - 1) fib + (n - 2) fib;
  end
where
  assert 0 fib ==> 0;
  assert 1 fib ==> 1;
  assert 2 fib ==> 1;
  assert 3 fib ==> 2;
  assert 4 fib ==> 3;
  assert 5 fib ==> 5;
  assert 6 fib ==> 8;
  assert 7 fib ==> 13;
  assert 8 fib ==> 21;
  assert 9 fib ==> 34;
  assert 10 fib ==> 55;
end

define main: _ = 9 fib;
