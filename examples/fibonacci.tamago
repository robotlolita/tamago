% language: tamago
module tamago.examples.fibonacci

use tamago.prelude as Prelude
open Prelude

function n fib =
  match n with
  | 0 => 0
  | 1 => 1
  | n => (n - 1) fib + (n - 2) fib
  end
where
  0 fib ==> 0;
  1 fib ==> 1;
  2 fib ==> 1;
  3 fib ==> 2;
  4 fib ==> 3;
  5 fib ==> 5;
  6 fib ==> 8;
  7 fib ==> 13;
  8 fib ==> 21;
  9 fib ==> 34;
  10 fib ==> 55


function main: _ =
  10 fib