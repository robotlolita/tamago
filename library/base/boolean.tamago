% language: tamago
/// Boolean values
namespace tamago::base::boolean

use tamago::native::unsafe::boolean as N;
use tamago::native::unsafe::types as Types;

define Boolean = Types.Boolean;

define self and that =
  N.and(self, that);

define self or that =
  N.or(self, that);

define not self =
  N.not(self);

define self === that =
  N.eq(self, that);

define self =/= that =
  N.not-eq(self, that);